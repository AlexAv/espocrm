{
    "fields": {
        "number": {
            "type": "autoincrement",
            "dbType": "bigint"
        },
        "event": {
            "type": "varchar",
            "maxLength": 100,
            "required": true,
            "view": "views/webhook/fields/event"
        },
        "target": {
            "type": "linkParent"
        },
        "data": {
            "type": "jsonObject"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "isProcessed": {
            "type": "bool"
        }
    },
    "links": {
        "target": {
            "type": "belongsToParent"
        }
    },
    "collection": {
        "orderBy": "number",
        "order": "desc"
    }
}
